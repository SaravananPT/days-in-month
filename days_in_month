def month(a):
  if a<13:
    if a == 1:
      print('31')
    if a == 2:
      print('28')
    if a == 3:
      print('31')
    if a == 4:
      print('30')
    if a == 5:
      print('31')
    if a == 6:
      print('30')
    if a == 7:
      print('31')
    if a == 8:
      print('31')
    if a == 9:
      print('30')
    if a == 10:
      print('31')
    if a == 11:
      print('30')
    if a == 12:
      print('31')
  else:
    print('Error')


month(int(input()))
